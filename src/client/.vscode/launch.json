{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "https://localhost:8080",
            "webRoot": "${workspaceFolder}/src",
            "preLaunchTask": "serve",
            "sourceMapPathOverrides": {
              "webpack:///src/*": "${webRoot}/*"
            },
        },
    ]
}